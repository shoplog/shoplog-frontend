{
	"extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
	"plugins": ["prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	}
}
